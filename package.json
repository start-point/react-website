{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.31.7",
    "lodash": "^4.17.21",
    "md-editor-rt": "^1.3.0",
    "parallax-js": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "17.x",
    "react-fullslip": "^1.2.3",
    "react-particles-js": "^3.6.0",
    "react-router-config": "^5.1.1",
    "react-tsparticles": "^1.37.5",
    "react-typewriter-hook": "^1.0.1",
    "redux-devtools-extension": "^2.13.9",
    "sanitize-html": "^2.6.1",
    "save": "^2.4.0",
    "swiper": "4.5",
    "umi": "^3.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
